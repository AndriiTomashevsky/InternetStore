@model ProductsListViewModel

<form method="get" asp-action="List" asp-controller="Product" class="search navbar-form">
	<div class="form-group">
		<input asp-for="@Model.Search" type="text" class="form-control" placeholder="Search" />
	</div>
	<button type="submit" class="btn btn-default">Search</button>
</form>
